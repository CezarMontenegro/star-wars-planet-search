{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["trybe-frontend"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".tsx", ".ts"] }]
  }
}
